{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health"
    },
    {
      "src": "/api/test-auth",
      "dest": "/api/test-auth"
    },
    {
      "src": "/_next/static/(?:[^/]+/)*[^/]+\\.(?:js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|otf)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}