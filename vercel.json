{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "includeFiles": [
          "next.config.js",
          "next.config.ts",
          "tsconfig.json"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/_next/static/(?:[^/]+/)*[^/]+\\.(?:js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|otf)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET", "HEAD", "OPTIONS"]
    }
  ]
}